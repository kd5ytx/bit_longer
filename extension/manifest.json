{
"manifest_version": 2,
    "name": "Bit Longer",
    "description": "Expands bit.ly links on Twitter to full un-shortened links",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "jquery-1.12.0.min.js",
                "jquery.appear.js",
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
        }

}
